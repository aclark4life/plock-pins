[addon]
packages =
    plonetheme.diazo_sunburst
    zope2_bootstrap
zcml = 
    zope2_bootstrap

[buildout]
extends = 
# The order matters. Zope/Plone versions go first
    https://raw.github.com/plock/pins/master/4.3.2-versions
# then we override as needed
    https://raw.github.com/plock/pins/master/base

[plock]
eggs = 
    ${base:packages}
    ${addon:packages}
    Plone

zcml =
    ${base:zcml}
    ${addon:zcml}
